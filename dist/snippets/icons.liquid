{%- comment -%}
    Inputs:
      icon: 'account',
      size: '64x64',
      fill: 'red',
      type: 'img',
      class: 'extra classes'
{%- endcomment -%}

{% comment %}theme-check-disable{% endcomment %}
{%- liquid
  if unit != blank
    assign object_unit = unit
  else
    assign object_unit = 'px'
  endif

  if size != blank
    assign object_size = size | split: 'x'
    assign object_size_w = object_size[0]
    assign object_size_h = object_size[1]
  endif

  if icon != blank
    assign object_icon = icon
  endif

  if fill != blank
    assign object_fill = fill
  endif

  if class != blank
    assign object_class = class
  endif

  if type != blank
    if type == 'svg'
      assign object_type = type
    else
      assign object_type = 'img'
    endif
  endif

  if flip != blank
    assign object_filp = flip
  else
    assign object_flip = false
  endif
-%}

